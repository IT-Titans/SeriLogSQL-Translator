<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SeriSQLTranslator</id>
    <version>1.0.0</version>
    <authors>IT Titans GmbH</authors>
    <description>SeriLogSQL Translator is a powerful and user-friendly tool designed to bridge the gap between SeriLog and SQL. Leveraging a robust parsing system, it transforms SeriLog log strings into executable SQL commands, eliminating the need for manual translation and making your database interactions more efficient and reliable.</description>
    <tags>logging, log, console, sql, efcore, entity framework</tags>
    <repository type="git" url="https://github.com/IT-Titans/SeriLogSQL-Translator" />
    <dependencies>
      <group targetFramework="net6.0">
        <dependency id="Serilog" version="3.0.1" exclude="Build,Analyzers" />
        <dependency id="Serilog.Extensions" version="2.2.2" exclude="Build,Analyzers" />
        <dependency id="Serilog.Sinks.Console" version="4.1.0" exclude="Build,Analyzers" />
        <dependency id="Serilog.Sinks.Console.LogThemes" version="0.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
